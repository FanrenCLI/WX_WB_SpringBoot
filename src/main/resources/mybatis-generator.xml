<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <!-- 一个context对应一个数据库,数据库链接URL，用户名、密码 -->
    <context id="MySQL" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/wx?useUnicode=true&amp;characterEncoding=utf-8&amp;serverTimezone=UTC"
                        userId="root"
                        password="660116">
        </jdbcConnection>

        <!--是否启用java.math.BigDecimal-->
        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!--指定Pojo生成的位置-->
        <javaModelGenerator targetPackage="com.fanren.wx.Pojo" targetProject=".\src\main\java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!--指定mapper.xml文件生成的位置-->
        <sqlMapGenerator targetPackage="Mapper"  targetProject=".\src\main\resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        <!--指定Dao生成的位置-->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.fanren.wx.Dao"
                             targetProject=".\src\main\java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <!--指定数据库中的表-->
        <table tableName="user" domainObjectName="User"></table>
        <!--<table tableName="brand" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">-->
    </context>
</generatorConfiguration>